{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cosmos": {
    "Databases": {
      "BlipsDatabase": {
        "DatabaseId": "BlipsDb",
        "Throughput": null,
        "Containers": {
          "Accounts": {
            "ContainerId": "Accounts",
            "PartitionKeyPath": "/accountId",
            "Throughput": null,
            "UniqueKeySets": [ [ "/email" ] ],
            "DefaultTtlSeconds": -1
          },
          "Blips": {
            "ContainerId": "Blips",
            "PartitionKeyPath": "/userId",
            "Throughput": null,
            "DefaultTtlSeconds": -1
          }
        }
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://+:443",
        "Protocols": "Http1AndHttp2"
      }
    }
  },
  "AllowedHosts": "*"
}
