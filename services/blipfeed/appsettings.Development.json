{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cosmos": {
    "Endpoint": "https://cosmos-sysdesign.documents.azure.com:443"
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://+:443",
        "Protocols": "Http1AndHttp2"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "/mnt/secrets/azure-aks-appgw-chain-pfx"
      }
    }
  },
  "BlipsCache": {
    "Enabled": true,
    "Redis": "localhost:6379,abortConnect=false",
    "InstanceName": "blips:",
    "FeedTtlSeconds": 10
  },
  "AllowedHosts": "*"
}
