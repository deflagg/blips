{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cosmos": {
    "Databases": {
      "UserAdmin": {
        "DatabaseId": "UserAdminDb",
        "Throughput": null,
        "Containers": {
          "Accounts": {
            "ContainerId": "Accounts",
            "PartitionKeyPath": "/accountId",
            "Throughput": null,
            "UniqueKeySets": [ [ "/email" ] ],
            "DefaultTtlSeconds": -1
          },
          "Profiles": {
            "ContainerId": "Profiles",
            "PartitionKeyPath": "/ProfileId",
            "Throughput": 400
          }
        }
      },
      "Audit": {
        "DatabaseId": "AuditDb",
        "Throughput": 400,
        "Containers": {
          "Events": { "ContainerId": "Events", "PartitionKeyPath": "/tenantId" },
          "Outbox": { "ContainerId": "Outbox", "PartitionKeyPath": "/pk", "ExcludeAllFromIndexing": true }
        }
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://+:443",
        "Protocols": "Http1AndHttp2"
      }
    }
  },
  "AllowedHosts": "*"
}
